//#include<iostream.h>
#include <stdio.h>
#include<math.h>
int main()
{
    int d,i,j,n;
    printf("Enter no. of data bits:  ");
    scanf("%d", &d);
    int r=0;
    while(pow(2,r)<=(d+r+1))
    {
        r++;
    }
    n=r+d;
    printf("Total no. of bits are %d", n);
    int a[11];
    for(i=0;i<11;i++)
    {
        a[i]=0;
    }
    for(i=0;i<r;i++)
    {
        j=pow(2,i);
        a[j]=1;
    }
    printf("\nEnter the data to be transmitted:  ");
    int data[10],x=0;
    for(i=0;i<d;i++)
    {
       scanf("%d", &data[i]);
    }
    for(i=11;i>0;i--)
    {
        if(a[i]==0)
        {
            a[i]=data[x];
            x++;
        }
    }
   
   /*for(i=11;i>0;i--)
   { 
       printf("%d", a[i]);
   }*/
   printf("\nDo you want to check for even parity or odd: If even press 1 if odd press 0: ");
   int ch;
   scanf("%d", &ch);
   //even parity
   if(ch==1)
   {
       //for r1
       int p1=a[1]+a[3]+a[5]+a[7]+a[9]+a[11];
       if(p1%2!=0)
       {
           j=pow(2,0);
           a[j]=0;
       }
       
       
       //for r2
       int p2=a[2]+a[3]+a[6]+a[7]+a[10]+a[11]; 
       if(p2%2!=0)
       {
           j=pow(2,1);
           a[j]=0;
       }
       
       //for r3
       int p3=a[4]+a[5]+a[6]+a[7]; 
       if(p3%2!=0)
       {
           j=pow(2,2);
           a[j]=0;
       }
       
       //for r4
       int p4=a[8]+a[9]+a[10]+a[11]; 
       if(p4%2!=0)
       {
           j=pow(2,3);
           a[j]=0;
       }
   }
   //odd parity
   else if(ch==0)
   {
       //for r1
       int p1=a[1]+a[3]+a[5]+a[7]+a[9]+a[11];
       if(p1%2==0)
       {
           j=pow(2,0);
           a[j]=0;
       }
       
       
       //for r2
       int p2=a[2]+a[3]+a[6]+a[7]+a[10]+a[11]; 
       if(p2%2==0)
       {
           j=pow(2,1);
           a[j]=0;
       }
       
       //for r3
       int p3=a[4]+a[5]+a[6]+a[7]; 
       if(p3%2==0)
       {
           j=pow(2,2);
           a[j]=0;
       }
       
       //for r4
       int p4=a[8]+a[9]+a[10]+a[11]; 
       if(p4%2==0)
       {
           j=pow(2,3);
           a[j]=0;
       }
   }
   printf("Sender word is:\n");
   for(i=11;i>0;i--)
   { 
       printf("%d ", a[i]);
   }
   printf("\nEnter the reciever word:  ");
   int b[11];
   for(i=11;i>0;i--)
   {
     scanf("%d", &b[i]);
   } 
   

//Error Detection
   printf("\nDo you want to check for even parity or odd(for reciever): \nIf even press 1 if odd press 0: ");
   int ch1;
   scanf("%d", &ch1);
   //even parity
   if(ch1==1)
   {
       //for r1
       int p1=b[1]+b[3]+b[5]+b[7]+b[9]+b[11];
       if(p1%2!=0)
       {
           j=pow(2,0);
           b[j]=1;
       }
       else
       {
           j=pow(2,0);
           b[j]=0;
       }
       
       
       //for r2
       int p2=b[2]+b[3]+b[6]+b[7]+b[10]+b[11]; 
       if(p2%2!=0)
       {
           j=pow(2,1);
           b[j]=1;
       }
       else
       {
           j=pow(2,1);
           b[j]=0;
       }
       
       
       //for r3
       int p3=b[4]+b[5]+b[6]+b[7]; 
       if(p3%2!=0)
       {
           j=pow(2,2);
           b[j]=1;
       }
       else
       {
           j=pow(2,2);
           b[j]=0;
       }
       
       
       
       //for r4
       int p4=b[8]+b[9]+b[10]+b[11]; 
       if(p4%2!=0)
       {
           j=pow(2,3);
           b[j]=1;
       }
       else
       {
           j=pow(2,3);
           b[j]=0;
       }
   }
   //odd parity
   else if(ch1==0)
   {
       //for r1
       int p1=b[1]+b[3]+b[5]+b[7]+b[9]+b[11];
       if(p1%2==0)
       {
           j=pow(2,0);
           b[j]=0;
       }
       else
       {
           j=pow(2,0);
           b[j]=1;
       }
       
       
       //for r2
       int p2=b[2]+b[3]+b[6]+b[7]+b[10]+b[11]; 
       if(p2%2==0)
       {
           j=pow(2,1);
           b[j]=0;
       }
       else
       {
           j=pow(2,1);
           b[j]=1;
       }
       
       
       
       //for r3
       int p3=b[4]+b[5]+b[6]+b[7]; 
       if(p3%2==0)
       {
           j=pow(2,2);
           b[j]=0;
       }
       else
       {
           j=pow(2,2);
           b[j]=1;
       }
       
       
       
       //for r4
       int p4=b[8]+b[9]+b[10]+b[11]; 
       if(p4%2==0)
       {
           j=pow(2,3);
           b[j]=0;
       }
       else
       {
           j=pow(2,3);
           b[j]=1;
       }
       
       
   }
   
   int r1,r2,r3,r4;
   j=pow(2,0);
   r1=b[j];
   j=pow(2,1);
   r2=b[j];
   j=pow(2,2);
   r3=b[j];
   j=pow(2,3);
   r4=b[j];
   
   int error_bit=0;
   error_bit=(r1*pow(2,0))+(r2*pow(2,1))+(r3*pow(2,2))+(r4*pow(2,3));
   printf("\nThe error is %dth bit", error_bit);
}




